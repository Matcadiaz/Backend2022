<% if(productos.length > 0){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow; text-align: center;"> <th>Producto</th> <th>Precio</th> <th>Imagen</th> </tr>
            <%productos.forEach((producto) => {%>
            <tr>
                <td style="text-align: center;"><%=producto.producto%></td>
                <td style="text-align: center;"><%=producto.precio%></td>
                <td>
                    <div style="text-align: center">
                        <img style="width: 80px; height:80px;" src="<%=producto.URL%>" alt="">
                    </div>
                </td>
            </tr>
            <%});%>
        </table>
    </div>
<% } else{ %>
    <h3 class="alert alert-danger">No se encontraron datos</h1>
<% } %>