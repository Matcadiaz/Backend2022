<%- include('../partials/encabezado')-%>
<body>
    <div class="container mt-3">
        <div class="jumbotron">
            <h1 style="color:blue;">Ingrese datos</h1>
            <br>
            <form action="/productos" method="post">                
                <div class="form-group">
                    <label for="nombre"><b>Producto</b></label>
                    <input id="nombre" class="form-control" type="text" name="producto">
                </div>
                <div class="form-group">
                    <label for="precio"><b>Precio</b></label>
                    <input id="apellido" class="form-control" type="text" name="precio">
                </div>
                <div class="form-group">
                    <label for="URL"><b>URL</b></label>
                    <input id="URL" class="form-control" type="text" name="URL">
                </div>
                <button class="btn btn-success mt-3 mb-5">Enviar</button>
                <button class="btn btn-success mt-3 mb-5"><a href="/productos">Ir a productos</a></button>
            </form>
            
        </div>
    </div>
</body>